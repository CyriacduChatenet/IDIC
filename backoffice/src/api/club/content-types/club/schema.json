{
  "kind": "collectionType",
  "collectionName": "clubs",
  "info": {
    "singularName": "club",
    "pluralName": "clubs",
    "displayName": "Club"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "phone": {
      "type": "integer",
      "required": true
    },
    "events": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event.event",
      "mappedBy": "club"
    },
    "players": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::player.player",
      "inversedBy": "clubs"
    }
  }
}
