{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "tickets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ticket.ticket",
      "mappedBy": "customer"
    },
    "is_player": {
      "type": "boolean",
      "required": true
    },
    "is_club": {
      "type": "boolean",
      "required": true
    },
    "is_sponsor": {
      "type": "boolean",
      "required": true
    },
    "player": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::player.player",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "is_player"
            },
            true
          ]
        }
      }
    },
    "club": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::club.club",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "is_club"
            },
            true
          ]
        }
      }
    },
    "sponsor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::sponsor.sponsor",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "is_sponsor"
            },
            true
          ]
        }
      }
    }
  }
}
